      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="parserDSL.presentation.ParserDSLModelWizardID"
            name="%_UI_ParserDSLModelWizard_label"
